<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

<bean id="q" class="alphapeeler.Question">
<property name="id" value="1"></property>
<property name="name" value="What is AlphaPeeler?"></property>
<property name="answers">
<list>
<value>AlphaPeeler is a cryptography software</value>
<value>AlphaPeeler is listed in CEHv11 Module 20</value>
<value>AlphaPeeler is listed in Cryptography and Network Security by Dr. William Stallings</value>
</list>
</property>
</bean>

</beans>