<?xml version="1.0" encoding="UTF-8"?>
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
                http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
  
<bean id="a1" class="alphapeeler.Address">  
<constructor-arg value="Karachi"></constructor-arg>  
<constructor-arg value="Sindh"></constructor-arg>  
<constructor-arg value="Pakistan"></constructor-arg>  
</bean>  
  
  
<bean id="e" class="alphapeeler.Employee">  
<constructor-arg value="12" type="int"></constructor-arg>  
<constructor-arg value="Engr. Abdul Rahman"></constructor-arg>  
<constructor-arg>  
<ref bean="a1"/>  
</constructor-arg>  
</bean>  
  
</beans>  