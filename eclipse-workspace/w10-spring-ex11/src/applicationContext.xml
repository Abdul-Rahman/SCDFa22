<?xml version="1.0" encoding="UTF-8"?>
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans   
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
  
<bean id="address1" class="alphapeeler.Address">  
<property name="addressLine1" value="21/3, Liaquat Avenue"></property>  
<property name="city" value="Model Colony"></property>  
<property name="state" value="Sindh"></property>  
<property name="country" value="Pakistan"></property>  
</bean>  
  
<bean id="obj" class="alphapeeler.Employee">  
<property name="id" value="1"></property>  
<property name="name" value="Hashim Kaliya"></property>  
<property name="address" ref="address1"></property>  
</bean>  
  
</beans>  